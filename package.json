{
  "name": "@itskyedo/eslint-config",
  "version": "0.1.1",
  "author": "Kyedo <hey@kyedo.dev> (https://github.com/itskyedo)",
  "description": "An opinionated configuration for ESLint.",
  "license": "MIT",
  "homepage": "https://github.com/itskyedo/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/itskyedo/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "config",
    "eslintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itskyedo/eslint-config.git"
  },
  "scripts": {
    "prepare": "husky",
    "watch": "parcel watch",
    "clean": "rimraf dist",
    "build": "npm run clean && parcel build --no-cache",
    "test": "echo \"Warning: No tests specified.\"",
    "typecheck": "tsc --noEmit",
    "formatcheck": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint --quiet .",
    "lintfix": "eslint --quiet --fix .",
    "check": "npm run formatcheck && npm run typecheck && npm run lint",
    "fix": "npm run format && npm run lintfix"
  },
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "targets": {
    "main": {
      "optimize": true,
      "sourceMap": false
    },
    "types": {
      "sourceMap": false
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@parcel/packager-ts": "^2.13.3",
    "@parcel/transformer-typescript-types": "^2.13.3",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.10.7",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "parcel": "2.13.3",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sort": "^4.0.0",
    "typescript-eslint": "^8.20.0"
  }
}
